package com.bridgelabz.Fundoo.Utility;

import org.apache.catalina.connector.Response;

public class UtilityImplementation implements UtilityInterface {
	
	public Response getResponse(int statusCode, String statusMessage) 
	{
		Response respone = new Response();
		respone.setStatus(statusCode,statusMessage);
	//	respone.setStatusMessage(statusMessage);
		return respone;
	}

	public Response getResponse(int statusCode, String token, String statusMessage) 
	{
		Response respone = new Response();
		respone.setStatus(statusCode);
		respone.setToken(token);
		respone.setStatusMessage(statusMessage);
		return respone;
	}
}


}
